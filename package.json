{
  "name": "react-virtual-draggable-grid",
  "version": "0.1.0",
  "description": "virtual, draggable list component built with react-virtualized and react-motion",
  "repository": {
    "type": "git",
    "url": "https://github.com/twils0/react-virtual-draggable-grid"
  },
  "author": "twils0",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/twils0/react-virtual-draggable-grid/issues"
  },
  "homepage": "https://github.com/twils0/react-virtual-draggable-grid#readme",
  "main": "app/index.jsx",
  "scripts": {
    "webpack": "webpack --progress --colors --mode production",
    "dev": "webpack-dev-server --mode development --open",
    "test": "jest",
    "test_coverage": "jest -- --coverage",
    "test_watch": "jest --watch"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "transform": {
      "\\.(jsx?)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/src/__mocks__/mockFile.js",
      "\\.(css|less)$": "<rootDir>/app/src/__mocks__/mockStyle.js"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "fast-equals": "^1.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-hot-loader": "^4.3.8",
    "react-motion": "^0.5.2"
  }
}
